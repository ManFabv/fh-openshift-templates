{
    "kind": "SecurityContextConstraints",
    "apiVersion": "v1",
    "metadata": {
        "name": "anyuid-with-chroot",
        "annotations": {
            "kubernetes.io/description": "anyuid-with-chroot provides all features of the anyuid SCC but allows chroot inside containers."
        }
    },
    "priority": 10,
    "allowPrivilegedContainer": false,
    "defaultAddCapabilities": null,
    "requiredDropCapabilities": [
        "KILL",
        "MKNOD"
    ],
    "allowedCapabilities": null,
    "allowHostDirVolumePlugin": false,
    "volumes": [
        "configMap",
        "downwardAPI",
        "emptyDir",
        "persistentVolumeClaim",
        "secret"
    ],
    "allowHostNetwork": false,
    "allowHostPorts": false,
    "allowHostPID": false,
    "allowHostIPC": false,
    "seLinuxContext": {
        "type": "MustRunAs"
    },
    "runAsUser": {
        "type": "RunAsAny"
    },
    "supplementalGroups": {
        "type": "RunAsAny"
    },
    "fsGroup": {
        "type": "RunAsAny"
    },
    "readOnlyRootFilesystem": false
}
